{
  "headers": [
    {
      "source": "/audio/(.*)",
      "headers": [
        { "key": "Accept-Ranges", "value": "bytes" },
        { "key": "Content-Type", "value": "audio/mp4" }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Content-Type-Options", "value": "nosniff" }
      ]
    }
  ]
}
